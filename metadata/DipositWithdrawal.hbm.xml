<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.mohbilling.model">

	<!-- DipositWithdrawal -->
	
	<class name="DipositWithdrawal" table="moh_bill_deposit_withdrawal">

		<id name="withdrawalID" type="int" column="withdrawal_id" unsaved-value="0">
			<generator class="native" />
		</id>

		<property name="withdrawalDate" column="withdrawal_date" type="java.util.Date" />
		<property name="withdrawalAmount" column="withdrawal_amount" type="big_decimal" />
		<property name="withdrawalReason" column="withdrawal_reason" type="java.lang.String" />
		<property name="createdDate" column="created_date" type="java.util.Date" />
		<property name="voided" column="voided" type="boolean" />
		<property name="voidedDate" column="voided_date" type="java.util.Date" />
		<property name="voidReason" column="void_reason" type="java.lang.String" />
		
		<many-to-one name="deposit" class="org.openmrs.module.mohbilling.model.Deposit" column="deposit_id" />	
		<many-to-one name="cashier" class="org.openmrs.User" column="cashier" />
		<many-to-one name="creator" class="org.openmrs.User" column="creator" />
		<many-to-one name="voidedBy" class="org.openmrs.User" column="voided_by" />

	</class>

</hibernate-mapping>
